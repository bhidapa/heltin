{
  "private": true,
  "name": "heltin-app",
  "engines": {
    "node": ">=16",
    "yarn": ">=1.22.0"
  },
  "packageManager": "yarn@3.2.4",
  "scripts": {
    "get-schema-enums": "yarn relay-enum-generator --schema ../cmd/graphql/schema.graphql --path ./src --name enums.graphql.ts && echo \"/* tslint:disable */\n/* eslint-disable */\n// @ts-nocheck\n\n$(cat src/enums.graphql.ts)\" > src/enums.graphql.ts",
    "relay": "relay-compiler",
    "relay:clean": "rimraf $(find . -type d -name __generated__)",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "type-check": "tsc --noEmit",
    "build": "yarn vite build",
    "start": "yarn vite"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@popperjs/core": "^2.11.6",
    "@tanstack/react-location": "^3.7.4",
    "@tippyjs/react": "^4.2.6",
    "date-fns": "^2.29.3",
    "detect-browser": "^5.3.0",
    "downshift": "^6.1.12",
    "focus-trap-react": "^10.0.0",
    "print-js": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.37.0",
    "react-intl": "^6.2.1",
    "react-popper": "^2.3.0",
    "react-relay": "^14.1.0",
    "react-toastify": "^9.0.8",
    "relay-runtime": "^14.1.0",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@trivago/prettier-plugin-sort-imports": "^3.3.1",
    "@types/babel__core": "^7.1.19",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-relay": "^14.1.2",
    "@types/relay-runtime": "^14.1.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitejs/plugin-react": "^2.1.0",
    "babel-plugin-relay": "^14.1.0",
    "browserslist": "^4.21.4",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "prettier": "^2.7.1",
    "relay-compiler": "^14.1.0",
    "relay-config": "^12.0.1",
    "relay-enum-generator": "^0.0.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vite-tsconfig-paths": "^3.5.1"
  }
}
