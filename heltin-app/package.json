{
  "name": "heltin-app",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhidapa/heltin.git"
  },
  "bugs": {
    "url": "https://github.com/bhidapa/heltin/issues"
