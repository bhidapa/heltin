  "engines": {
    "node": ">=12",
    "yarn": ">=1.22.4"
  },
  "scripts": {
    "get-schema": "get-graphql-schema http://localhost:5000/graphql --json > ./schema.json && node scripts/graphql/fix-schema.js ./schema.json",
    "relay": "relay-compiler",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "type-check": "tsc --noEmit",
    "build:dll": "webpack --config webpack.dll.config.js --hide-modules --display-chunks --color --progress",
    "build": "webpack -p --config webpack.prod.config.js --display-optimization-bailout --profile --progress",
    "start:hot": "webpack-dev-server --config webpack.dev.config.js --progress",
    "start": "yarn build:dll && yarn start:hot"
  },
  "dll": {
    "name": "vendors",
    "path": "node_modules/.dll"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
